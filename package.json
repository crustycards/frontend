    "@babel/preset-typescript": "7.16.0",
    "@types/aes-js": "3.1.1",
    "@types/amqplib": "0.8.2",
    "@types/cookie": "0.4.1",
    "@types/cookie-parser": "1.4.2",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.13",
    "@types/google-protobuf": "3.15.5",
    "@types/history": "4.7.9",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.6",
