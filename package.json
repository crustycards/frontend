    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "ts-jest": "26.5.6",
    "ts-loader": "9.2.6",
    "ts-proto": "1.86.0",
    "tslint": "6.1.3",
    "typescript": "4.4.4",
    "webpack": "5.64.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "testURL": "http://localhost/"
  }
}
