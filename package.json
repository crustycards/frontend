{
  "name": "github-fetcher-fullstack-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-dev-windows": "npm run build-proto-windows && npm run build-grpc-windows && webpack -d --watch --mode development",
    "build-dev-linux": "npm run build-proto-linux && npm run build-grpc-linux && webpack -d --watch --mode development",
    "build-prod-windows": "npm run build-proto-windows && npm run build-grpc-windows && webpack --mode production",
    "build-prod-linux": "npm run build-proto-linux && npm run build-grpc-linux && webpack --mode production",
    "start": "ts-node src/server/main.ts",
    "start-dev": "nodemon",
    "check-typescript-types": "tsc --noEmit --skipLibCheck",
    "lint": "tslint 'src/**/**/*.ts' 'src/**/**/*.tsx'",
    "lint-clean": "npm run lint -- --fix",
    "test": "jest",
    "deploy": "git pull && npm install && webpack && pm2 restart all",
    "update-proto-repo": "git submodule foreach git pull origin master",
    "build-proto-windows": "mkdir proto-gen-out & protoc --plugin=./node_modules/.bin/protoc-gen-ts.cmd --plugin=./node_modules/.bin/protoc-gen-grpc.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --ts_out=proto-gen-out --proto_path=./cards-proto ./cards-proto/api/*.proto google/api/client.proto google/api/field_behavior.proto google/api/resource.proto google/api/annotations.proto google/api/http.proto",
    "build-proto-linux": "mkdir -p proto-gen-out && protoc --plugin=./node_modules/.bin/protoc-gen-ts.cmd --plugin=./node_modules/.bin/protoc-gen-grpc.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --ts_out=proto-gen-out --proto_path=./cards-proto ./cards-proto/api/*.proto google/api/client.proto google/api/field_behavior.proto google/api/resource.proto google/api/annotations.proto google/api/http.proto",
    "build-grpc-windows": "mkdir proto-gen-out & grpc_tools_node_protoc --plugin=./node_modules/.bin/grpc_tools_node_protoc_plugin.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --grpc_out=grpc_js:./proto-gen-out --proto_path=./cards-proto google/api/field_behavior.proto google/api/resource.proto ./cards-proto/api/model.proto ./cards-proto/api/cardpack_service.proto ./cards-proto/api/user_service.proto ./cards-proto/api/game_service.proto",
    "build-grpc-linux": "mkdir -p proto-gen-out && grpc_tools_node_protoc --plugin=./node_modules/.bin/grpc_tools_node_protoc_plugin --js_out=import_style=commonjs,binary:./proto-gen-out --grpc_out=grpc_js:./proto-gen-out --proto_path=./cards-proto google/api/field_behavior.proto google/api/resource.proto ./cards-proto/api/model.proto ./cards-proto/api/cardpack_service.proto ./cards-proto/api/user_service.proto ./cards-proto/api/game_service.proto"
  },
  "author": "Thomas Volk",
  "license": "BOII",
  "devDependencies": {
    "@babel/preset-typescript": "7.12.7",
    "@types/aes-js": "3.1.1",
    "@types/amqplib": "0.5.16",
    "@types/cookie": "0.4.0",
    "@types/cookie-parser": "1.4.2",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.9",
    "@types/google-protobuf": "3.7.4",
    "@types/history": "4.7.8",
    "@types/jest": "26.0.19",
    "@types/jsonwebtoken": "8.5.0",
    "@types/passport-google-oauth20": "2.0.5",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-infinite-scroller": "1.2.1",
    "@types/react-redux": "7.1.12",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.6",
    "@types/react-swipeable-views": "0.13.0",
    "@types/react-test-renderer": "17.0.0",
    "@types/socket.io": "2.1.12",
    "@types/socket.io-client": "1.4.34",
    "@types/webpack": "4.41.25",
    "babel-loader": "8.2.2",
    "babel-plugin-transform-imports": "2.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "fork-ts-checker-webpack-plugin": "6.0.7",
    "grpc_tools_node_protoc_ts": "5.0.1",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "ts-proto": "1.41.1",
    "tslint": "6.1.3",
    "typescript": "4.1.3",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "4.3.0",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {
