    "dotenv": "8.2.0",
    "express": "4.17.1",
    "google-protobuf": "3.14.0",
    "grpc-tools": "1.10.0",
    "history": "4.10.1",
    "jsonwebtoken": "8.5.1",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "protoc-gen-grpc": "1.4.4",
    "react": "17.0.1",
