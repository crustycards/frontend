    "url-loader": "4.1.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "testURL": "http://localhost/"
  }
}
