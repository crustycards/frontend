{
  "name": "github-fetcher-fullstack-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-dev": "npm run generate-stubs && webpack -d --watch --mode development",
    "build-prod": "npm run generate-stubs && webpack --mode production",
    "start": "ts-node src/server/main.ts",
    "start-dev": "nodemon",
    "test": "jest",
    "lint": "tslint 'src/**/*.ts' 'src/**/*.tsx'",
    "lint-clean": "npm run lint -- --fix",
    "check-typescript-types": "tsc --noEmit --skipLibCheck",
    "generate-stubs": "npm run generate-grpc && npm run generate-proto",
    "generate-grpc": "mkdir -p proto-gen-out && grpc_tools_node_protoc --plugin=./node_modules/.bin/grpc_tools_node_protoc_plugin --js_out=import_style=commonjs,binary:./proto-gen-out --grpc_out=grpc_js:./proto-gen-out --proto_path=./proto google/api/field_behavior.proto google/api/resource.proto ./proto/crusty_cards_api/model.proto ./proto/crusty_cards_api/cardpack_service.proto ./proto/crusty_cards_api/user_service.proto ./proto/crusty_cards_api/game_service.proto",
    "generate-proto": "mkdir -p proto-gen-out && protoc --plugin=./node_modules/.bin/protoc-gen-ts.cmd --plugin=./node_modules/.bin/protoc-gen-grpc.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --ts_out=proto-gen-out --proto_path=./proto ./proto/crusty_cards_api/*.proto google/api/client.proto google/api/field_behavior.proto google/api/resource.proto google/api/annotations.proto google/api/http.proto"
  },
  "author": "Thomas Volk",
  "license": "BOII",
  "dependencies": {
    "@grpc/grpc-js": "1.4.4",
    "@grpc/proto-loader": "0.6.6",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@nestjs/common": "8.2.0",
    "@nestjs/core": "8.2.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.2.0",
    "@nestjs/platform-socket.io": "8.2.0",
    "@nestjs/websockets": "8.2.0",
    "aes-js": "3.1.2",
    "amqplib": "0.8.0",
    "async": "3.2.2",
    "axios": "0.24.0",
    "body-parser": "1.19.0",
    "connected-react-router": "6.9.1",
    "cookie": "0.4.1",
    "cookie-parser": "1.4.5",
    "dotenv": "9.0.2",
    "express": "4.17.1",
    "google-protobuf": "3.19.1",
    "grpc-tools": "1.11.2",
    "history": "4.10.1",
    "jsonwebtoken": "8.5.1",
    "passport": "0.5.0",
    "passport-google-oauth20": "2.0.0",
    "protoc-gen-grpc": "1.4.4",
    "react": "17.0.2",
    "react-dnd": "14.0.4",
    "react-dnd-html5-backend": "14.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "10.2.2",
    "react-infinite-scroller": "1.2.4",
    "react-redux": "7.2.6",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-redux": "4.0.8",
    "react-swipeable-views": "0.14.0",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.2",
    "reflect-metadata": "0.1.13",
