{
  "name": "github-fetcher-fullstack-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-dev-windows": "npm run build-proto-windows && npm run build-grpc-windows && webpack -d --watch --mode development",
    "build-dev-linux": "npm run build-proto-linux && npm run build-grpc-linux && webpack -d --watch --mode development",
    "build-prod-windows": "npm run build-proto-windows && npm run build-grpc-windows && webpack --mode production",
    "build-prod-linux": "npm run build-proto-linux && npm run build-grpc-linux && webpack --mode production",
    "start": "ts-node src/server/main.ts",
    "start-dev": "nodemon",
    "lint": "tslint 'src/**/**/*.ts' 'src/**/**/*.tsx'",
    "lint-clean": "npm run lint -- --fix",
    "test": "jest",
    "deploy": "git pull && npm install && webpack && pm2 restart all",
    "update-proto-repo": "git submodule foreach git pull origin master",
    "build-proto-windows": "mkdir proto-gen-out & protoc --plugin=./node_modules/.bin/protoc-gen-ts.cmd --plugin=./node_modules/.bin/protoc-gen-grpc.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --ts_out=proto-gen-out --proto_path=./cards-proto ./cards-proto/api/*.proto ./cards-proto/game/*.proto google/api/client.proto google/api/field_behavior.proto google/api/resource.proto google/api/annotations.proto google/api/http.proto",
    "build-proto-linux": "mkdir -p proto-gen-out && protoc --plugin=./node_modules/.bin/protoc-gen-ts.cmd --plugin=./node_modules/.bin/protoc-gen-grpc.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --ts_out=proto-gen-out --proto_path=./cards-proto ./cards-proto/api/*.proto ./cards-proto/game/*.proto google/api/client.proto google/api/field_behavior.proto google/api/resource.proto google/api/annotations.proto google/api/http.proto",
    "build-grpc-windows": "mkdir proto-gen-out & grpc_tools_node_protoc --plugin=./node_modules/.bin/grpc_tools_node_protoc_plugin.cmd --js_out=import_style=commonjs,binary:./proto-gen-out --grpc_out=./proto-gen-out --proto_path=./cards-proto google/api/field_behavior.proto google/api/resource.proto ./cards-proto/api/model.proto ./cards-proto/api/cardpack_service.proto ./cards-proto/api/user_service.proto ./cards-proto/game/game_service.proto",
    "build-grpc-linux": "mkdir -p proto-gen-out && grpc_tools_node_protoc --plugin=./node_modules/.bin/grpc_tools_node_protoc_plugin --js_out=import_style=commonjs,binary:./proto-gen-out --grpc_out=./proto-gen-out --proto_path=./cards-proto google/api/field_behavior.proto google/api/resource.proto ./cards-proto/api/model.proto ./cards-proto/api/cardpack_service.proto ./cards-proto/api/user_service.proto ./cards-proto/game/game_service.proto"
  },
  "author": "Thomas Volk",
  "license": "BOII",
  "devDependencies": {
    "@types/amqplib": "0.5.13",
    "@types/cookie": "0.4.0",
    "@types/cookie-parser": "1.4.2",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.6",
